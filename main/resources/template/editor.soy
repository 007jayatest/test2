/**** Excellentable Message Container ****/
<div id="euiMessagePanel"></div>

/**** Formula Bar ****/
<table id="euiFormulaBar" style="position:relative;width: 100%;">
    <tr>
        <td>
            <input type="text" id="euiPositionBox" disabled="disabled"
                style="text-align: center; height: 18px;margin: 0;float:left;width:8%;margin-right:5px;"></input>
            <textarea id="euiFormulaBox" spellcheck="false" style="float:left;width:88%;line-height:16px;"/>
        </td>
    </tr>
</table>

/**** Main Container ****/
<div id="euiControlPanel" style="bottom: 0;">
    <div class="eui-edit-spread" spellcheck="false" style="width:100%;">
        <div id="euiPrintArea"></div>
    </div>
</div>
<div class="eui-print"></div>
<table id="euiStatusBar" style="width: 100%;height: auto;padding-top:6px;">
    <tr>
        <td style="background-color: #217346;">
            <span id="euiRapidInputMode" style="font-family: 'Segoe UI'; font-size: 9pt; margin-left:16px; color: white" />
        </td>
        <td style="width: 300px; background-color: #217346;">
            <input type="range" min="25" max="400" value="100" step="5"
                style="width: 100%; margin: 0;border: none; padding: 0;" onchange="zoomFactor_Changed(this.value/100)" />
        </td>
        <td>
            <input id="euiOpenFile" hidden="hidden" type="file" style="visibility: collapse" accept=".xls, .xlsx, .csv, .ods"/>
            <input id="euiSpreadContentChanged" type="hidden" value="false">
            <form id="euiExport" action = "{$contextPath}/rest/excellentable/1.0/export"
                class="aui" method = "post" enctype="multipart/form-data">
                <input type="hidden" id="euiExportType" name="exportType"/>
                <input type="hidden" id="euiDataVal" name="data"/>
                <input type="hidden" id="euiExportFileNameVal" name="exportFileName"/>
                <button class ="aui-button exportToExcel" style="float:left;">Export</button>
            </form>
        </td>
    </tr>
</table>


/**** For edit history ****/
<div id="euiWrapper" class="eui-wrapper"></div>
    <nav id="euiMenuPushRight" class="eui-menu eui-menu-push-right">
        <div class="eui-version-header" style="background:green;">
            <button class="eui-version-menu-bar">
            <span class="icon-icomoon icon-history" id="euiVersionMenuBarHeaderIcon"></span>
                 {getText('com.addteq.confluence.plugin.excellentable.version.history')}
            </button>
        </div>
        <div class="eui-version-history-container">
            <ul class="eui-version-menu-items"></ul>
        </div>
    </nav>
    <div id="euiVersionMask" class="eui-version-mask">
        <div class="eui-version-topBar">
            <span title="{getText('com.addteq.confluence.plugin.excellentable.dialog.close')}"
                class="eui-version-menu-close aui-icon aui-icon-small aui-iconfont-close-dialog"></span>
        </div>
        <div id="euiVersionSpread"></div>
    </div>
/**** End for edit history ****/

/** Sheet Delete warning dialog that asks user confirmation to delete **/
<section id="sheet-delete-warning-dialog" class="aui-dialog2 aui-dialog2-medium aui-dialog2-warning aui-layer" role="dialog" aria-hidden="true">
    <header class="aui-dialog2-header">
        <h2 class="aui-dialog2-header-main">{getText('com.addteq.confluence.plugin.excellentable.confirmsheetdeletion')}</h2>
        <a class="aui-dialog2-header-close">
            <span class="aui-icon aui-icon-small aui-iconfont-close-dialog">Close</span>
        </a>
    </header>
    <div class="aui-dialog2-content">
        <p>{getText('com.addteq.confluence.plugin.excellentable.sheetdeleteareyousure')}</p>
    </div>
    <footer class="aui-dialog2-footer">
        <div class="aui-dialog2-footer-actions">
            <button id="sheet-delete-confirm-button" class="aui-button aui-button-primary">
            {getText('com.addteq.confluence.plugin.excellentable.delete.sheet')}</button>
        </div>
    </footer>
</section>

/**** EUI Tabstrip *****/
<div id="eui-tab-strip">
    /*   Sheet Tab Context Menu. */
    <button class="aui-button aui-dropdown2-trigger aui-dropdown2-trigger-arrowless eui-dropdown hidden" id="eui-tab-context-menu-trigger" aria-controls="eui-tab-context-menu">Sheet Tab Context Menu</button>
    <aui-dropdown-menu id="eui-tab-context-menu" data-aui-alignment-container="#eui-tab-strip">
        <aui-section class="eui-dropdownmenu">
            <aui-item-link data-name="deletesheet" data-action="remove"><span class="aui-icon aui-icon-small aui-iconfont-delete"></span>&nbsp;
                {getText('com.addteq.confluence.plugin.excellentable.context.menu.delete')}
            </aui-item-link>

            <aui-item-link data-name="renamesheet" data-action="rename"><span class="aui-icon aui-icon-small aui-iconfont-build"></span>&nbsp;
                {getText('com.addteq.confluence.plugin.excellentable.context.menu.rename')}
            </aui-item-link>

            <aui-item-link data-name="protectsheet" data-action="protect"><span class="aui-icon aui-icon-small aui-iconfont-locked"></span>&nbsp;
                {getText('com.addteq.confluence.plugin.excellentable.context.menu.protect')}
            </aui-item-link>

        </aui-section>
     </aui-dropdown-menu>
    <div class="eui-tab-strip-container">
        <div class="eui-tab-strip-dd hidden">
            <ul class="eui-menu-list menu-list eui-tab-nav" data-nav-type="menu">
            </ul>         
        </div>
        <div class="eui-tab-strip-scrollbar">
        </div>
        <div class="eui-tab-strip-controls">
            <span id="euiTabstripAddSheet" class="aui-icon aui-icon-small aui-iconfont-add eui-tab-strip-icon" title="{getText('com.addteq.confluence.plugin.excellentable.tabstrip.newsheet')}">{getText('com.addteq.confluence.plugin.excellentable.tabstrip.newsheet')}</span>
            <span id="euiTabstripOptions" class="aui-icon aui-icon-small aui-iconfont-menu eui-tab-strip-icon" title="{getText('com.addteq.confluence.plugin.excellentable.tabstrip.allsheets')}">{getText('com.addteq.confluence.plugin.excellentable.tabstrip.allsheets')}</span>
        </div>
        <div class="eui-tab-strip-tabs">
            <div class="aui-tabs horizontal-tabs">
                <ul class="eui-tabs-menu tabs-menu eui-tab-nav" data-nav-type="tabs">
                </ul>
            </div>
        </div>
        <div class="eui-scrolloptions hidden">
            <span id="euiScrollLeft" class="fa fa-caret-left eui-tab-strip-icon"></span>
            <span id="euiScrollRight" class="fa fa-caret-right eui-tab-strip-icon"></span>
        </div>
    </div>
</div>


