{namespace Confluence.Templates.Excellentable}
/******************************************
 *  Excellentable Edit mode template.
 *  @param contextPath
 *  @param isMac
 *******************************************/
{template .Spread}

<!-- Class first-item and last-item are used to give rounded corner to the selected items in the context menu to match the dialog corner.
    Addition of new menu items in future shall keep the first-item and last-item classes at it's respective place for example,
    - items can be added in middle Or
    - any item added at top or bottom shall have the first-item and last-item as class name and no other items.
    - Merge and Unmerge are the last items because those appear one at a time.
-->

/**** Spread Context Menu ****/
<aui-dropdown-menu id="euiSpreadContextMenu" role="menu" style="left: 832px; top: 411px; display: none;">
    <aui-section>
        <aui-item-link data-action="cut" name="cut" class="eui-first-item">
            <span class="icon-icomoon icon-scissors icomoon-icon-small"></span>&nbsp;{getText('com.addteq.confluence.plugin.excellentable.context.menu.cut')}
        </aui-item-link>
        <aui-item-link data-action="copy" name="copy">
            <span class="aui-icon aui-icon-small aui-iconfont-copy-clipboard"></span>&nbsp;{getText('com.addteq.confluence.plugin.excellentable.context.menu.copy')}
        </aui-item-link>
        <aui-item-link data-action="paste" name="paste">
            <span class="icon-icomoon icon-clipboard"></span>&nbsp;{getText('com.addteq.confluence.plugin.excellentable.context.menu.paste')}
        </aui-item-link>
        <aui-item-link data-action="unlink" name="unlink">
            <span class="aui-icon aui-icon-small aui-iconfont-unlink"></span>&nbsp;{getText('com.addteq.confluence.plugin.excellentable.context.menu.unlink')}
        </aui-item-link>
        <aui-item-link class="context-menu-trigger" for="euiPasteSpecial">
            <span class="icon-icomoon icon-paste"></span>
            &nbsp;{getText('com.addteq.confluence.plugin.excellentable.context.menu.paste.special')}
        </aui-item-link>
        <aui-item-link data-action="insert" name="insert" class="eui-context-header" style="display: none;">
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.insert')}
        </aui-item-link>
        <aui-item-link data-action="delete" name="delete" class="eui-context-header" style="display: none;">
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.delete')}
        </aui-item-link>
        <aui-item-link class="eui-divider"></aui-item-link>
        <aui-item-link data-action="merge" name="merge" class="eui-context-cell eui-context-merge eui-last-item" style="display: none;">
            <span class="aui-icon aui-icon-small aui-iconfont-table-merge"></span>&nbsp;
                {getText('com.addteq.confluence.plugin.excellentable.context.menu.merge')}
        </aui-item-link>
        <aui-item-link data-action="unmerge" name="unmerge" class="eui-context-cell eui-context-unmerge eui-last-item" style="display: none;">
            <span class="aui-icon aui-icon-small aui-iconfont-table-split"></span>&nbsp;
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.unmerge')}
        </aui-item-link>
        <aui-item-link data-action="comment" name="comment">
            <span class="icon-icomoon icon-bubbles2"></span>&nbsp;
            {getText('com.addteq.confluence.plugin.excellentable.note')}
        </aui-item-link>
        <aui-item-link data-action="protectcell" class="eui-last-item" name="protectcell">
            <span class="aui-icon aui-icon-small aui-iconfont-locked"></span>&nbsp;
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.protect.cells')}
        </aui-item-link>

        <aui-item-link data-action="conditionalformatting" class="context-menu-trigger eui-last-item" for="euiConditionalFormat" name="conditionalFormatting">
            <span class="aui-icon aui-icon-small aui-iconfont-new-edit"></span>&nbsp;
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.conditionalformatting')}
        </aui-item-link>
    </aui-section>
</aui-dropdown-menu>


/**** paste special ***/

<aui-dropdown-menu id="euiPasteSpecial" role="menu">
    <aui-section>
        <aui-item-link data-action="pastevaluesonly" name="pastevaluesonly" class="eui-first-item">
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.paste.special.values')|noAutoescape}
        </aui-item-link>
        <aui-item-link data-action="pasteformatsonly" name="pasteformatsonly">
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.paste.special.format')|noAutoescape}
        </aui-item-link>
        <aui-item-link data-action="pasteformulaonly" name="pasteformulaonly">
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.paste.special.formula')|noAutoescape}
        </aui-item-link>
        <aui-item-link data-action="Pastallexceptborders" name="Pastallexceptborders" class="eui-last-item">
            {getText('com.addteq.confluence.plugin.excellentable.context.menu.paste.special.except.border')|noAutoescape}
        </aui-item-link>
    </aui-section>
</aui-dropdown-menu>

/**** Border Dialog ***/
<section id="euiBorderFormat" class="aui-dialog2 aui-dialog2-small aui-layer" role="dialog" aria-hidden="true">
    <header class="aui-dialog2-header">
        <h2 class="aui-dialog2-header-main">{getText('com.addteq.confluence.plugin.excellentable.border')}</h2>
        <a class="aui-dialog2-header-close">
            <span class="aui-icon aui-icon-small aui-iconfont-close-dialog">Close</span>
        </a>
    </header>
    <div class="aui-dialog2-content">
    <h4>Type:&nbsp;<a href="https://www.addteq.com/exc/user-guide/edit-mode/how-to-use-borders" target="_blank" style="color:#52b052;" title="{getText('com.addteq.confluence.plugin.excellentable.help')}">
                      <span class="aui-icon aui-icon-small aui-iconfont-help">{getText('com.addteq.confluence.plugin.excellentable.help')}</span>
                    </a>
    </h4>
        <div class="tablecontainer">
            <svg id="euisvgtable" class="normal eui-border eui-border-type default">
                <line class="default" for="border" data-src="top" x1="6" x2="200" y1="6" y2="6" />
                <line class="default" for="border" data-src="innerHorizontal" x1="6" x2="200" y1="53" y2="53" />
                <line class="default" for="border" data-src="bottom" x1="6" x2="200" y1="100" y2="100" />
                <line class="default" for="border" data-src="left" x1="6"  x2="6" y1="6"y2="100" />
                <line class="default" for="border" data-src="innerVertical" x1="103" x2="103" y1="6" y2="100" />
                <line class="default" for="border" data-src="right" x1="200" x2="200" y1="6" y2="100" />
                <line class="eui-background" data-src="top" x1="6" x2="200" y1="6" y2="6" />
                <line class="eui-background" data-src="innerHorizontal" x1="6" x2="200" y1="53" y2="53" />
                <line class="eui-background" data-src="bottom" x1="6" x2="200" y1="100" y2="100" />
                <line class="eui-background" data-src="left" x1="6"  x2="6" y1="6" y2="100" />
                <line class="eui-background" data-src="innerVertical" x1="103" x2="103" y1="6" y2="100" />
                <line class="eui-background" data-src="right" x1="200" x2="200" y1="6" y2="100" />
            </svg>
        </div>
        <table class="eui-border borderoptions">
        <tr>
        <td>
            <h4>Style:&nbsp;</h4>
        </td>
        <td>
            <button class="aui-button aui-dropdown2-trigger eui-border-type" id="euiLineStyle" aria-controls="euisvgtablestyle"><svg class="eui-border eui-border-type"><line x1="0" x2="200" y1="10" y2="10" /></svg></button>
        </td>
        </tr>
        <tr>
        <td>
            <h4>Color:</h4>
        </td>
        <td>
        <div><input type='text' id="togglePaletteOnly" /></div>
        </td>
        </tr>
        <tr>
        <td>
        </td>
        <td><button class="aui-button" id="euiBorderClear">Reset Defaults</button></td>
        </tr>
        </table>


        <aui-dropdown-menu id="euisvgtablestyle" class="eui-border">
          <aui-section>
            <aui-item-link class="eui-border-type" title="Thin" checked><svg class="eui-border"><line class="thin" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Medium"><svg class="eui-border"><line class="medium" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Dashed"><svg class="eui-border"><line class="dashed" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Dotted"><svg class="eui-border"><line class="dotted" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Thick"><svg class="eui-border"><line class="thick" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Hair"><svg class="eui-border"><line class="hair" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Medium Dashed"><svg class="eui-border"><line class="mediumDashed" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Dash Dot"><svg class="eui-border"><line class="dashDot" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Medium Dash Dot"><svg class="eui-border"><line class="mediumDashDot" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Dash Dot Dot"><svg class="eui-border"><line class="dashDotDot" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
            <aui-item-link class="eui-border-type" title="Medium Dash Dot Dot"><svg class="eui-border"><line class="mediumDashDotDot" x1="0" x2="200" y1="10" y2="10" /></svg></aui-item-link>
          </aui-section>
         </aui-dropdown-menu>
    </div>
</section>


/**** Data Validation Dialog ***/
<div id="euiDataValidationDialog" class="eui-menu-dialog" title="Data Validation">
    <ul>
        <li>
            <a href="#euiTabValidationSettings">Settings</a>
        </li>
        <li>
            <a href="#euiTabValidationInputMessage" id="euiValidationInputMessge">Input Message</a>
        </li>
        <li>
            <a href="#euiTabValidationErrorAlert" id="euiValidationErrorAlert">Error Alert</a>
        </li>
    </ul>
    <div id="euiTabValidationSettings" class="eui-tab-validation-color">
        <div class="eui-data-validation-dialog-subdiv">
            <div>
                <span>Validation criteria</span>
            </div>
            <div class="eui-subdiv">
                <span>Allow:</span>
            </div>
            <div class="eui-subdiv">
                <select id="euiValidatorTypes">
                    <option value="AnyValidator" selected>Any value</option>
                    <option value="NumberValidator">Number</option>
                    <option value="ListValidator">List</option>
                    <option value="FormulaListValidator">FormulaList</option>
                    <option value="DateValidator">Date</option>
                    <option value="TextLengthValidator">Text length</option>
                    <option value="FormulaValidator">Custom</option>
                </select>
                <input id="euiChkValidatorIgnoreBlank" type="checkbox" class="eui-subdiv" />
                <label for="euiChkValidatorIgnoreBlank" class="wijmo-wijribbon-checkbox">IgnoreBlank</label>
            </div>
            <div style="margin-top: 10px; display:none" id="euiDataTitle">
                <span>Data:</span>
            </div>
            <div id="euiValidatorTab1" class="eui-subdiv">
                <select id="euiValidatorComparisonOperator">
                    <option value="6" selected>Between</option>
                    <option value="7">NotBetween</option>
                    <option value="0">EqualTo</option>
                    <option value="1">NotEqualTo</option>
                    <option value="2">GreaterThan</option>
                    <option value="4">LessThan</option>
                    <option value="3">GreaterThanOrEqualTo</option>
                    <option value="5">LessThanOrEqualTo</option>
                </select>
                <div class="eui-subdiv">
                    <span id="euiRangeStart"></span>
                    <input id="euiTxtValidatorValue1" placeholder="Value1" class="eui-txt-validator-value">
                </div>
                <div class="eui-subdiv">
                    <span id="euiRangeEnd"></span>
                    <input id="euiTxtValidatorValue2" placeholder="Value2" class="eui-txt-validator-value">
                </div>
                <div id="euiIsTimeTab" class="eui-subdiv">
                    <input id="euiChkIsTime" type="checkbox" />
                    <label for="euiChkIsTime" class="wijmo-wijribbon-checkbox"> Is Time </label>
                </div>
                <div id="euiIsIntTab" class="eui-subdiv">
                    <input id="euiChkIsInteger" type="checkbox" />
                    <label for="euiChkIsInteger" class="wijmo-wijribbon-checkbox"> Is Integer </label>
                </div>
            </div>
            <div id="euiValidatorTab2" class="eui-subdiv">
                <span id="euiRangeValidator"></span>
                <input id="euiTxtValidatorValue" placeholder="Value1">
                <span id="euiPrompt"></span>
            </div>
        </div>
    </div>
    <div id="euiTabValidationInputMessage"  class="eui-tab-validation-color">
        <div class="eui-data-validation-dialog-subdiv">
            <div>
                <input id="euiChkShowMessage" type="checkbox" />
                <label for="euiChkShowMessage" class="wijmo-wijribbon-checkbox">
                        Show input message when cell is selected.
                </label>
            </div>
            <div class="eui-menu-dialog-subdiv">
                <span>When cell is selected,show this input message:</span>
            </div>
            <div style="margin-top: 20px; margin-left: 10px">
                <div>Title:</div>
                <input id="euiTxtMessageTitle" placeholder="MessageTitle" style="width:88%" />
            </div>
            <div style="margin-top: 20px; margin-left: 10px">
                <div>Message:</div>
                <textarea rows="5" id="euiTxtMessageMessage" placeholder="Message" style="width:88%"></textarea>
            </div>
        </div>
    </div>
    <div id="euiTabValidationErrorAlert" class="eui-tab-validation-color">
        <div class="eui-data-validation-dialog-subdiv">
            <div>
                <input id="euiChkShowError" type="checkbox" />
                <label for="euiChkShowError" class="wijmo-wijribbon-checkbox">show error alert after invalid data is entered</label>
            </div>
            <div class="eui-menu-dialog-subdiv">
                <span>When the user enters invalid data,show this error alert:</span>
            </div>
            <div class="eui-menu-dialog-subdiv">
                <select id="euiValidatorErrorStyles">
                    <option value="0" selected>Stop</option>
                    <option value="1">Warning</option>
                    <option value="2">Information</option>
                </select>
            </div>
            <div class="eui-menu-dialog-subdiv">
                <div>Title:</div>
                <input id="euiTxtErrorTitle" placeholder="ErrorTitle" style="width:88%" />
            </div>
            <div class="eui-menu-dialog-subdiv">
                <div>Message:</div>
                <textarea rows="5" id="euiTxtErrorMessage" placeholder="ErrorMessage" style="width:88%" ></textarea>
            </div>
        </div>
    </div>
</div>



/**** Conditional Formatting ****/
<div id="euiConditionalFormatDialog" class="eui-menu-dialog" title="Formatting Rule">
    <div style="margin-left: 5px; margin-top: 3px;">
        <div style="margin-left: 5px; margin-top: 10px">
            <span id="euiRuleText" style="font-weight:bold">Format only cells with:</span>
        </div>
        <div style="margin-left: 5px; margin-top: 10px">
            <span id="euiColorScaleStyle" style="width: 40px; display: none; display: -moz-inline-box;">Style:</span>
            <select id="euiRule1">
                <option value="0" selected>Cell Value</option>
                <option value="1">Specific Text</option>
                <option value="2">Dates Occurring</option>
                <option value="3">Formula</option>
                <option value="4">Top10</option>
                <option value="5">Unique</option>
                <option value="6">Duplicate</option>
                <option value="7">Average</option>
                <option value="8">2-Color Scale</option>
                <option value="9">3-Color Scale</option>
            </select>
            <select id="euiComparisonOperator1" style="margin-left: 3px">
                <option value='0' selected>EqualsTo</option>
                <option value='1'>NotEqualsTo</option>
                <option value='2'>GreaterThan</option>
                <option value='3'>GreaterThanOrEqualsTo</option>
                <option value='4'>LessThan</option>
                <option value='5'>LessThanOrEqualsTo</option>
                <option value='6'>Between</option>
                <option value='7'>NotBetween</option>
            </select>
            <input type="input" id="euiValue1" style="width: 60px; margin-left: 3px">
            <span id="euiAndText" style="display: none; margin-left: 3px">and</span>
            <input type="input" id="euiValue2" style="width: 60px; margin-left: 3px; display: none">
            <span id="euiFormatText" style="display: none; margin-left: 5px"></span>
        </div>
        <div id="euiColorScale" style="display:block; margin-left: 5px; margin-top: 16px;">
            <div style="margin-left: 40px">
                <span>Minimum</span>
                <span id="euiMidPoint" style="margin-left: 65px">Midpoint</span>
                <span style="margin-left: 65px">Maximum</span>
            </div>
            <div class="eui-subdiv">
                <span class="eui-conditionalformatdialog-span" class="eui-conditional-format-dialog-span">Type:</span>
                <select id="euiMinType" class="eui-conditional-format-dialog-color-scale">
                    <option value="0">Number</option>
                    <option value="1" selected>LowestValue</option>
                    <option value="3">Percent</option>
                    <option value="4">Percentile</option>
                    <option value="6">Formula</option>
                </select>
                <select id="euiMidType" class="eui-conditional-format-dialog-color-scale">
                    <option value="0">Number</option>
                    <option value="3">Percent</option>
                    <option value="4" selected>Percentile</option>
                    <option value="6">Formula</option>
                </select>
                <select id="euiMaxType" class="eui-conditional-format-dialog-color-scale">
                    <option value="0">Number</option>
                    <option value="2" selected>HighestValue</option>
                    <option value="3">Percent</option>
                    <option value="4">Percentile</option>
                    <option value="6">Formula</option>
                </select>
            </div>
            <div class="eui-subdiv">
                <span class="eui-conditional-format-dialog-span">Value:</span>
                <input id="euiMinValue" type="text" value="" class="eui-conditional-format-dialog-input" />
                <input id="euiMidValue" type="text" value="50" class="eui-conditional-format-dialog-input" />
                <input id="euiMaxValue" type="text" value="" class="eui-conditional-format-dialog-input" />
            </div>
            <div class="eui-subdiv">
                <span class="eui-conditional-format-dialog-span">Color:</span>
                <input type="button" id="euiMinColor" value="" class="eui-conditional-format-dialog-color-scale"
                    title="minColor" onclick="setColor(event, 'Min Color')" />
                <input type="button" id="euiMidColor" value="" class="eui-conditional-format-dialog-color-scale"
                    title="midColor" onclick="setColor(event, 'Mid Color')" />
                <input type="button" id="euiMaxColor" value="" class="eui-conditional-format-dialog-color-scale"
                    title="maxColor" onclick="setColor(event, 'Max Color')" />
            </div>
        </div>
    </div>
</div>


/**** Data Bar Dialog ****/
<div id="euiDatabarDialog" class="eui-menu-dialog" title="Data Bars">
    <div style="margin-top: 4px; margin-right: 5px; border:1px solid gray; padding: 8px">
        <div>
            <b>Value</b>
        </div>
        <div style="height:16px">
            <div style="width: 100px; position:absolute; left:70px; text-align:center">Minimum</div>
            <div style="width: 100px; position:absolute; left:220px; text-align:center">Maximum</div>
        </div>
        <div class="eui-databar-dialog-subdiv">
            <span>Type:</span>
            <select id="euiMinimumType" style="width: 100px; position:absolute; left:70px; height: 22px">
                <option value="0">Number</option>
                <option value="1">LowestValue</option>
                <option value="3">Percent</option>
                <option value="4">Percentile</option>
                <option value="5" selected="selected">Automin</option>
                <option value="6">Formula</option>
            </select>
            <select id="euiMaximumType" style="width: 100px; position:absolute; left:220px; height: 22px">
                <option value="0">Number</option>
                <option value="2">HighestValue</option>
                <option value="3">Percent</option>
                <option value="4">Percentile</option>
                <option value="6">Formula</option>
                <option value="7" selected="selected">Automax</option>
            </select>
        </div>
        <div style="margin-top: 16px;">
            <span >Values:</span>
            <input id="euiMinimumValue" type="text" style="width: 96px; position:absolute; left:70px;" placeholder="minimumValue">
            <input id="euiMaximumValue" type="text" style="width: 96px; position:absolute; left:220px;" placeholder="maximumValue">
        </div>
        <div style="margin-top: 18px">
            <b>Appearance</b>
        </div>
        <div class="eui-databar-dialog-subdiv">
            <input type="checkbox" id="euiShowBarOnly" />
            <label for="euiShowBarOnly">Show Bar Only</label>
        </div>
        <div class="eui-databar-dialog-subdiv">
            <input type="checkbox" id="euiGradient" />
            <label for="euiGradient" class="eui-data-bar-dialog-label">Gradient</label>
            <span style="width: 80px; display: inline-block; display: -moz-inline-box;">Color:</span>
            <input type="button" id="euiColor" value="" title="barcolor" style="width:80px; background:#638EC6"
                onclick="setColor(event, 'Bar Color')" />
        </div>
        <div class="eui-databar-dialog-subdiv">
            <input type="checkbox" id="euiShowBorder" />
            <label for="euiShowBorder" class="eui-data-bar-dialog-label">
                {getText('com.addteq.confluence.plugin.excellentable.comment.border.show.label')}</label>
            <span style="width: 80px; display: inline-block; display: -moz-inline-box;">
                {getText('com.addteq.confluence.plugin.excellentable.comment.border.euiColor.text')}</span>
            <input type="button" id="euiBorderColor" value="" title="barbordercolor" style="width:80px; background:#000000"
                 onclick="setColor(event, 'Bar Border Color')" />
        </div>
        <div class="eui-subdiv">
            <span>Bar Direction:</span>
            <select id="euiDataBarDirection" style="height: 22px">
                <option value="0" selected="selected">Left-to-Right</option>
                <option value="1">Right-to-Left</option>
            </select>
        </div>
        <div class="eui-databar-dialog-subdiv">
            <input type="checkbox" id="euiUseNegativeFillColor" />
            <label for="euiUseNegativeFillColor" class="eui-data-bar-dialog-label">
                Use Negative Fill Color</label>
            <span class="eui-databar-dialog-span">Negative Fill Color:</span>
            <input type="button" id="euiNegativeFillColor" value="" title="barnegativefillcolor" style="width:50px; background:#FF0000"
                onclick="setColor(event, 'Bar Negative Fill Color')" />
        </div>
